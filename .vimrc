set number
set t_Co=256
syntax on
colo torte
set cursorline
hi CursorLine cterm=none ctermbg=DarkMagenta ctermfg=White
set cursorcolumn
hi CursorColumn cterm=none ctermbg=DarkMagenta ctermfg=White
set ignorecase
set hlsearch
hi Search cterm=reverse ctermbg=none ctermfg=none
set incsearch
set cindent
set expandtab
set tabstop=4
set softtabstop=4
set shiftwidth=4
set mouse=a
set encoding=utf-8
